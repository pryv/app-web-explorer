{
  "name": "bootstrap-vue-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "vue-cli-service build --no-clean",
    "lint": "vue-cli-service lint",
    "start": "vue-cli-service serve",
    "test": "testcafe chrome tests/*.test.js --app \"yarn start\"",
    "setup": "sh scripts/setup.sh",
    "upload": "sh scripts/upload.sh",
    "clear": "rm -rf dist/ && yarn setup",
    "webserver": "node node_modules/rec-la/webserver/main.js",
    "webserver-test": "node node_modules/.bin/rec-la ./tests/webpage-auth/ 5443"
  },
  "dependencies": {
    "bootstrap-vue": "^2.1.0",
    "core-js": "^3.6.5",
    "pryv": "^2.0.3",
    "rec-la": "^0.1.14",
    "vee-validate": "^3.3.6",
    "vue": "^2.6.11",
    "vue-axios": "^2.1.5",
    "vue-cookies": "^1.7.2",
    "vue-json-pretty": "^1.6.5",
    "vue-router": "^3.2.0",
    "vuex": "^3.5.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.4.0",
    "@vue/cli-plugin-eslint": "~4.4.0",
    "@vue/cli-plugin-router": "^4.4.5",
    "@vue/cli-service": "~4.4.0",
    "babel-eslint": "^10.1.0",
    "bootstrap": "^4.3.1",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^6.2.2",
    "popper.js": "^1.16.0",
    "portal-vue": "^2.1.6",
    "sass": "^1.19.0",
    "sass-loader": "^8.0.0",
    "vue-cli-plugin-bootstrap-vue": "~0.6.0",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
